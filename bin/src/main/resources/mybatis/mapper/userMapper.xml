<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biz4up.bizsalad.mapper.UserMapper"> 
 	<select id="login" resultType="com.biz4up.bizsalad.UserVO">
 		select * from springboots.usertest where uid=#{uid}
 	</select>
 	
 	<insert id="insert">
 		insert into springboots.usertest(uid,upw,uname)
 		values(#{uid},#{upw},#{uname})
 	</insert>
</mapper>